CLASS
 NAME "< 44 - Convection peu probable"
 GROUP "TOTALX_FR-LINEAR"
 EXPRESSION ([pixel] >= 0 AND [pixel] < 44)
 STYLE
  COLORRANGE 247 247 247 43 131 186
  DATARANGE 0.00 44.0
 END
END


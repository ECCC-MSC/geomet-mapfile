CLASS
 NAME "<= -40"
 GROUP "TEMPWINTER-LINEAR"
 EXPRESSION ([pixel] < -40)
 STYLE
  COLORRANGE 0 0 127 0 0 127
  DATARANGE -100 -40.0 
 END
END

CLASS
 NAME "-40 -35"
 GROUP "TEMPWINTER-LINEAR"
 EXPRESSION ([pixel] >= -40 AND [pixel] < -35)
 STYLE
  COLORRANGE 0 0 127 0 0 203
  DATARANGE -40.0 -35.0
 END
END 


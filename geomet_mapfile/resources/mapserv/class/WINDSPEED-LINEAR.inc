CLASS
 NAME "0 10 km/hr"
 GROUP "WINDSPEEDKMH-LINEAR"
 EXPRESSION ([pixel] >= 0.00 AND [pixel] < 2.78)
 STYLE
  COLORRANGE 0 0 127 0 0 203
  DATARANGE 0.0 2.78
 END
END


CLASS
 NAME "<= -15"
 GROUP "TEMPSUMMER-LINEAR"
 EXPRESSION ([pixel] < -15)
 STYLE
  COLORRANGE 0 0 127 0 0 127
  DATARANGE -15.0 -100.0
 END
END 
 
 CLASS
 NAME "-15 -10"
 GROUP "TEMPSUMMER-LINEAR"
 EXPRESSION ([pixel] >= -15 AND [pixel] < -10)
 STYLE
  COLORRANGE 0 0 127 0 0 203
  DATARANGE -15.0 -10.0
 END
END 


CLASS
 NAME "0 20 km/hr"
 GROUP "GUST_MS2KMH-LINEAR"
 EXPRESSION ([pixel] >= 0.00 AND [pixel] < 5.56)
 STYLE
  COLORRANGE 0 0 127 0 0 234
  DATARANGE 0.0 5.56
 END
END


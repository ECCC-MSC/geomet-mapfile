CLASS
 NAME "<= 500 mb"
 GROUP "PRESSURE"
 EXPRESSION ([pixel] < 500.00)
 STYLE
  COLORRANGE 0 0 127 0 0 127
  DATARANGE 500.0 200.0
 END
END

CLASS
 NAME "500 610 mb"
 GROUP "PRESSURE"
 EXPRESSION ([pixel] >= 500.00 AND [pixel] < 610.00)
 STYLE
  COLORRANGE 0 0 127 0 63 254
  DATARANGE 500.0 610.0
 END
END


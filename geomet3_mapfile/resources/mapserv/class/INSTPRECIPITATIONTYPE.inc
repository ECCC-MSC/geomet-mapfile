CLASS
 NAME "Rain"
 GROUP "INSTPRECIPITATIONTYPE"
 EXPRESSION ([pixel] >= 1.0 AND [pixel] < 2.0)
 STYLE
  COLOR 0 221 0
 END
END

CLASS
 NAME "Rain-Snow"
 GROUP "INSTPRECIPITATIONTYPE"
 EXPRESSION ([pixel] >= 2.0 AND [pixel] < 3.0)
 STYLE
  COLOR 238 170 0
 END
END

CLASS
 NAME "Ice-Pellets"
 GROUP "INSTPRECIPITATIONTYPE"
 EXPRESSION ([pixel] >= 3.0 AND [pixel] < 4.0)
 STYLE
 COLOR 255 0 255
 END
END

CLASS
 NAME "Freezing-Rain"
 GROUP "INSTPRECIPITATIONTYPE"
 EXPRESSION ([pixel] >= 4.0 AND [pixel] < 5.0)
 STYLE
 COLOR 255 0 0
 END
END

CLASS
 NAME "Snow"
 GROUP "INSTPRECIPITATIONTYPE"
 EXPRESSION ([pixel] >= 5.0 AND [pixel] < 6.0)
 STYLE
 COLOR 0 238 255
 END
END
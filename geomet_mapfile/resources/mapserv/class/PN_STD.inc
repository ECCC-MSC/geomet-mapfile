CLASS
 NAME "0 60"
 GROUP "PN_STD"
 EXPRESSION ([pixel] >= 0 AND [pixel] < 60)
 STYLE
  COLOR 0 0 127
 END
END

CLASS
 NAME "60 75"
 GROUP "PN_STD"
 EXPRESSION ([pixel] >= 60 AND [pixel] < 75)
 STYLE
  COLOR 0 0 254
 END
END

CLASS
 NAME "75 100"
 GROUP "PN_STD"
 EXPRESSION ([pixel] >= 75 AND [pixel] < 100)
 STYLE
  COLOR 0 253 255
 END
END

CLASS
 NAME "100 135"
 GROUP "PN_STD"
 EXPRESSION ([pixel] >= 100 AND [pixel] < 135)
 STYLE
  COLOR 255 255 0
 END
END

CLASS
 NAME ">= 135"
 GROUP "PN_STD"
 EXPRESSION ([pixel] >= 135)
 STYLE
  COLOR 127 0 0
 END
END  

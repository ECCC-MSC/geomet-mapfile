CLASS
 NAME "1 - 5"
 GROUP "SNOWMASS-LINEAR"
 EXPRESSION ([pixel] >= 1 AND [pixel] < 5)
 STYLE
  COLORRANGE 0 0 127 0 0 255
  DATARANGE 1 5
 END
END


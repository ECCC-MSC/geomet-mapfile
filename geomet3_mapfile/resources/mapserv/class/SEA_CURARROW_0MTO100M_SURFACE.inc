CLASS
 NAME "0.00 0.10 m/s"
 GROUP "SEA_CURARROW_0MTO100M_SURFACE"
 EXPRESSION ([uv_length] >= 0.00 AND [uv_length] < 0.10)
 STYLE
  SYMBOL "arrow_wind"
  COLOR 153 232 253
   ANGLE [uv_angle]
  SIZE 10
 END

CLASS
 NAME "0.0 mm/h"
 GROUP "RADARURPPRECIPR"
 EXPRESSION ([pixel] < 0.1)
 STYLE
  COLOR 0 0 0
  OPACITY 0
 END
END
CLASS
 NAME "0.1 - 1mm/h"
 GROUP "RADARURPPRECIPR"
 EXPRESSION ([pixel] >= 0.1 AND [pixel] < 1)
 STYLE
  COLOR 153 204 255
 END
END


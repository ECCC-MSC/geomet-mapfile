CLASS
 NAME "0 km/hr"
 GROUP "WINDARROWKMH"
 EXPRESSION ([u] = 270)
 STYLE
  COLOR 0 0 127
  SYMBOL "circle_wind"
  SIZE 12
 END
END

CLASS
 NAME "0 10 km/hr"
 GROUP "WINDARROWKMH"
 EXPRESSION ([v] > 0.00 AND [v] < 2.78)
 STYLE
  COLOR 0 0 127
  SYMBOL "arrow_wind"
   ANGLE [u]
  SIZE 12
 END
END

